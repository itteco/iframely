    },
    "license": "MIT",
    "dependencies": {
        "@adobe/helix-fetch": "^3.0.9",
        "async": "^2.6.4",
        "cheerio": "^0.22.0",
        "chokidar": "^3.5.2",
        "ejs": "^3.1.7",
        "entities": "1.1.1",
        "express": "^4.18.0",
        "globby": "^12.0.2",
        "graceful-cluster": "0.0.3",
        "htmlparser2": "^7.1.2",
        "iconv-lite": "^0.6.3",
        "jslint": "^0.12.1",
        "jsontoxml": "0.0.11",
        "memcached": "2.2.2",
        "moment": "2.29.2",
        "node-cache": "1.*",
        "parse-iso-duration": "^1.1.0",
