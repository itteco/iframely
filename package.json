    },
    "license": "MIT",
    "dependencies": {
        "async": "2.4.1",
        "cheerio": "0.22.0",
        "chokidar": "^2.0.4",
        "ejs": "2.5.7",
        "entities": "1.1.1",
        "express": "^4.16.3",
        "graceful-cluster": "0.0.3",
        "htmlparser2": "3.9.2",
        "http-parser-js": "itteco/http-parser-js#magicode-fix-22",
        "iconv-lite": "0.4.17",
        "imagesize": "1.0.0",
        "jslint": "^0.12.0",
        "jsontoxml": "0.0.11",
        "memcached": "2.2.2",
        "moment": "2.19.3",
